{
  "name": "object-as-dictionary",
  "version": "0.0.3",
  "description": "utility for creating dictionary objects",
  "keywords": [
    "flow",
    "type-check",
    "dictionary",
    "table",
    "map",
    "__proto__",
    "null",
    "Object.create(null)"
  ],
  "author": "Irakli Gozalishvili <rfobic@gmail.com> (http://jeditoolkit.com)",
  "homepage": "https://github.com/Gozala/object-as-dictionary",
  "main": "./lib/index.js",
  "devDependencies": {
    "babel": "5.8.23",
    "babel-plugin-flow-comments": "1.0.9",
    "flow-bin": "0.17.0"
  },
  "babel": {
    "sourceMaps": "inline"
  },
  "scripts": {
    "test": "flow check",
    "build-node": "babel ./src --out-dir ./lib --plugins flow-comments --blacklist flow",
    "build-browser": "babel ./src --out-dir ./dist --modules umdStrict",
    "build": "npm run build-node && npm run build-browser",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Gozala/object-as-dictionary.git",
    "web": "https://github.com/Gozala/object-as-dictionary"
  },
  "bugs": {
    "url": "https://github.com/Gozala/object-as-dictionary/issues/"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/Gozala/object-as-dictionary/License.md"
    }
  ]
}
